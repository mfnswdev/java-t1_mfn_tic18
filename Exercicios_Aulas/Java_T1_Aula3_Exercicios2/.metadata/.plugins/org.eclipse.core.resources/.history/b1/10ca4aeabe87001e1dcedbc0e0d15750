package exerciciosDeDados;

public class dadosAreaRetangulo {
	private
	int base;
	int altura;
	
	public static int CalculaArea(int base, int altura) {
		return base * altura;
	}
	
public static void main(String[] args) {
	int base, altura;
	base = 3;
	altura = 7;
	
	System.out.println("A área do retangulo é igual à " + CalculaArea(base, altura));
}
}